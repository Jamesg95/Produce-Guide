{
  "name": "produce-guide",
  "version": "1.0.0",
  "description": "This is a website for a fictional produce information site.",
  "main": "script.js",
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "bootstrap": "^4.3.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "imagemin-cli": "^3.0.0",
    "lite-server": "^2.3.0",
    "node-sass": "^4.12.0",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.1",
    "rimraf": "^2.6.2",
    "usemin-cli": "^0.5.1"
  },
  "scripts": {
    "lite": "lite-server",
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run imagemin && npm run usemin",
    "scss": "node-sass -o css/ css/",
    "imagemin": "imagemin assets/images/* -o dist/img ",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html",
    "clean": "rimraf dist",
    "watch:scss": "onchange \"css/*.scss\" -- npm run scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jamesg95/Produce-Guide.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jamesg95/Produce-Guide/issues"
  },
  "homepage": "https://github.com/Jamesg95/Produce-Guide#readme"
}
